<%- include components/header %>
<%- include components/navbar %>

    <div class="testimonials-area section-padding-100">
        <div class="popular-course-details-area wow fadeInUp" data-wow-delay="300ms">
            <div class="single-top-popular-course d-flex align-items-center flex-wrap">
                <div class="popular-course-content">
                    <h5><%= filiere.nom %></h5>
                    <p><%= filiere.description %></p>
                </div>
                <div class="popular-course-content">
                    <h5>List des modules</h5>

                    <% modules.forEach(module => { %>
                        <div class="module">
                            <i class="fas fa-book-open"></i>
                            <span style="display:inline-block"><%= module.nom %></span>
                        </div>
                    <% }) %>

                </div>
            </div>
        </div>
    </div>

    <%- include components/footer %>

    <!-- jQuery-2.2.4 js -->
    <script src="/js/jquery/jquery-2.2.4.min.js"></script>
    <!-- Popper js -->
    <script src="/js/bootstrap/popper.min.js"></script>
    <!-- Bootstrap js -->
    <script src="/js/bootstrap/bootstrap.min.js"></script>
    <!-- All Plugins js -->
    <script src="/js/plugins/plugins.js"></script>
    <!-- Active js -->
    <script src="/js/active.js"></script>
    <!-- Google Maps -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAwuyLRa1uKNtbgx6xAJVmWy-zADgegA2s"></script>
    <script src="/js/google-map/map-active.js"></script>
</body>
</html>