<%- include components/header %>

<header>
    <nav>
        <div class="row">
            <a href="/"><img src="resources/img/logo.png" class="logo"></a>
            <a href="/"><img src="resources/img/logo - white.png"  class="logo-black"></a>
            <ul class="main-nav js--main-nav">
                <li><a href="/">ACCEUIL</a></li>
                <li><a href="/">ACTUALITES</a></li>
                <li><a href="/">COURS/EXERCICES</a></li>
                <li><a href="/">COMPTE</a></li>
                <% if(locals.user) { %>
                    <li><a href="/logout"><%= user.username %></a></li>
                <% } %>
            </ul>
            <a class="mobile-nav-icon js--nav-icon"><i class="ion-navicon-round"></i></a>
        </div>    
    </nav>
            
    <div class="hero-text-box">
        <h1>BIENVENUE A L'ISTAG BAB TIZIMI.</h1>
        <% if(!locals.user) { %>
            <a class="btn btn-full js--scroll-to-plans" href="/login">CONNEXION</a>
            <a class="btn btn-ghost js--scroll-to-start" href="/register">INSCRIPTION</a>
        <% } %>
        
    </div>
</header>

<section class="section-intro">
    <div class="row">
        <h2>ISTAG BAB TIZIMI</h2>
        <img id="istag" src="resources/img/istag.jpg"/>
        <div id="brief-intro">
            <h3>Brief Introduction</h3>
            <p>Istag bab tizimi est une institue de Technologie Applique et de gestion basé sur meknes , suivie le grand réseau des institue au maroc connais par OFPPT</p>
        </div>
        
    </div>
</section>

<section class="section-actualites">
    <h2>ACTUALITES</h2> 
    <% posts.forEach(post => { %>
        <div class="row news">
            <img width="30px" height="30px" style="float:right" src="./resources/img/new.png">
            <a style="border-bottom: none;" href="#"><i class="fas fa-arrow-alt-circle-right icon-click"></i></a>
            <span style="display:inline-blo ;margin-bottom:10px"><%= post.title %></span>
            <P style="margin-bottom:10px"><%= post.content %></P>
        </div>
    <% }) %>
</section>

