            <div class="row">

                <% secteurs.forEach(secteur => { %>
                    <div class="col-12 col-sm-6 col-lg-4">
                        <div class="single-course-area d-flex align-items-top mb-100 wow fadeInUp" data-wow-delay="700ms">
                            <div class="course-icon">
                                <i class="icon-worldwide"></i>
                            </div>
                            <div class="course-content">
                                <a href="/filieres?secteur=<%= secteur.code %>"><h4><%= secteur.name %></h4></a>
                                <p><%= secteur.description %></p>
                            </div>
                        </div>
                    </div>
                <% }) %>
               
            </div>
